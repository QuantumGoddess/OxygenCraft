{
  "schemaVersion": 1,
  "id": "oxygencraft",
  "version": "${version}",

  "name": "Oxygen Craft",
  "description": "A space exploration mod for minecraft",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/QuantumGoddess/OxygenCraft"
  },

  "license": "GPL-3.0",
  "icon": "assets/oxygencraft/icon.png",

  "environment": "*",
  "entrypoints": {
